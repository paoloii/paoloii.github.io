---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>paolo i.</title>
	<link href="https://paoloii.github.io/atom.xml" rel="self"/>
	<link href="https://paoloii.github.io/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://paoloii.github.io/</id>
	<author>
		<name>paolo i.</name>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://paoloii.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://paoloii.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
